                            ajaxCall('@Url.Action("ChangePasswordUser", "Dashboard")', { id: Id, newPassword: newPassword, currentPassword: currentPassword }, function (resp) {
                                if (resp.IsSuccess === true) {
                                    $('#lblMessageConfirmPassword').show();
                                    $('#lblMessageConfirmPassword').text("Password changed successfully");
                                    $('#currentpassword').val('');
                                    $('#newPassword').val('');
                                    $('#confirmPwd').val('');
                                }
                                else {
                                    $('#lblMessageConfirmPassword').show();
                                    $('#lblMessageConfirmPassword').text("Password does't change for some reason please try again.");
                                }
                            }, function (fail) {
                                //alert(fail);
                            }, function (err) {
                                //alert(err);
                            });
                        }
                        else {
                            $('#lblMessageConfirmPassword').show();
                            $('#lblMessageConfirmPassword').text("Password doesn't match");
                        }
                    }
                    }
                }
    </script>




